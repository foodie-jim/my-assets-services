{
  "name": "my-assets-services",
  "author": "Jim Kim <hjman.kim@gmail.com> (http://https://my-assets-runner-rypg66fkzq-uc.a.run.app)",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "dev": "concurrently \"npm:nodemon:start\" \"npm:nodemon:swagger\"",
    "nodemon:start": "cross-env NODE_ENV=development nodemon --exec ts-node src/app.ts",
    "nodemon:swagger": "nodemon --watch 'src/**/*.ts' -x tsoa spec",
    "predev": "npm run swagger",
    "prebuild": "npm run swagger",
    "build": "tsc -p .",
    "swagger": "tsoa spec"
  },
  "keywords": [],
  "license": "GPL-3.0",
  "dependencies": {
    "@types/morgan": "^1.9.3",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.1.6",
    "tsoa": "^3.9.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^16.6.0",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.24.0",
    "nodemon": "^2.0.12",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
